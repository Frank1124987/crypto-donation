 useRouter()\r\n        const route = useRoute()\r\n\r\n        const logout = () => {\r\n            const auth = getAuth();\r\n            signOut(auth).then(() => {\r\n                    // Sign-out successful.\r\n                    router.push('/')\r\n                }).catch((error) => {\r\n                // An error happened.\r\n            });\r\n        }\r\n\r\n        const login = ref(true)\r\n\r\n        const auth = getAuth()\r\n        onBeforeMount(() => {\r\n            onAuthStateChanged(auth, (user) => {\r\n                if ( !user ){\r\n                    login.value = true\r\n                }else{\r\n                    login.value = false\r\n                }\r\n            })\r\n        })\r\n\r\n        return {\r\n            logout,\r\n            login\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.navbar-option-container{\r\n    background-color: white;\r\n    border-radius: 32px;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-evenly;\r\n\r\n    width: 40%;\r\n    height: 38px;\r\n}\r\n\r\n.navbar-option{\r\n    height: 100%;\r\n    text-align: center;\r\n    line-height: 38px;\r\n\r\n    width: calc((100% - 15px)/4);\r\n    border-radius: 32px;\r\n    cursor: pointer;\r\n}\r\n\r\n.navbar-option:hover{\r\n    background-color: rgb(240, 240, 240);\r\n\r\n}\r\n\r\n.navbar-option-seperation{\r\n    border-left: 1px solid gray;\r\n    margin: 2px;\r\n    height: 15px;\r\n}\r\n</style>"],"sourceRoot":""}]}