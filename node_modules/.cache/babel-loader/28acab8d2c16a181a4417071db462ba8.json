st router = useRouter()\n        const route = useRoute()\n\n        const logout = () => {\n            const auth = getAuth();\n            signOut(auth).then(() => {\n                    // Sign-out successful.\n                    router.push('/')\n                }).catch((error) => {\n                // An error happened.\n            });\n        }\n\n        const login = ref(true)\n\n        const auth = getAuth()\n        onBeforeMount(() => {\n            onAuthStateChanged(auth, (user) => {\n                if ( !user ){\n                    login.value = true\n                }else{\n                    login.value = false\n                }\n            })\n        })\n\n        return {\n            logout,\n            login\n        }\n    }\n}\n</script>\n\n<style scoped>\n.navbar-option-container{\n    background-color: white;\n    border-radius: 32px;\n    display: flex;\n    align-items: center;\n    justify-content: space-evenly;\n\n    width: 40%;\n    height: 38px;\n}\n\n.navbar-option{\n    height: 100%;\n    text-align: center;\n    line-height: 38px;\n\n    width: calc((100% - 15px)/4);\n    border-radius: 32px;\n    cursor: pointer;\n}\n\n.navbar-option:hover{\n    background-color: rgb(240, 240, 240);\n\n}\n\n.navbar-option-seperation{\n    border-left: 1px solid gray;\n    margin: 2px;\n    height: 15px;\n}\n</style>"],"sourceRoot":""}]}