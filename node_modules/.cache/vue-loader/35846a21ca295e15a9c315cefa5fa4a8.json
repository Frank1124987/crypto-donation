   }).then((result) => {\n                console.log(\"result\",result)\n                contract.methods.getPlan(route.params.departmentId, plan.value.name).call().then(console.log)\n            }).catch(console.log)\n        }\n        return {\n            plan,\n            userId: computed(() => store.state.userId),\n            amount,\n            donate\n        }\n    }\n}\n</script>\n\n<style scoped>\n.plan{\n    padding : 2%;\n    border-style: solid;\n    margin: 1% 0 1% 0;\n}\n</style>"]}]}