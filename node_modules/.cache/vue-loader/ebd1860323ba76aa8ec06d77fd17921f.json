departmentName = ref(\"\")\n    const email = ref(\"\")\n    const phone = ref(\"\")\n    const category = ref(\"\")\n    \n    \n\n    const register = async () => {\n        const address = await web3.eth.getAccounts()\n        contract.methods.registerDepartment(departmentId.value, departmentName.value)\n            .send({\n                from : address[0],\n                gas : 9187500,\n        }).then(() => {\n            router.replace('/')\n        })\n    }\n\n    return {\n        departmentName,\n        departmentId,\n        email,\n        phone,\n        category,\n        address: computed(() => store.state.user.address),\n        register\n    }\n  },\n}\n</script>"]}]}